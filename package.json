  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ext 'ts,json,html' --exec 'npm run build && npm run start'",
    "build": "tsc && cp -r src/views dist/",
    "start": "node dist/index.js"
