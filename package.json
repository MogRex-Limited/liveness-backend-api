    "axios": "^1.9.0",
    "express": "^4.21.2",
    "form-data": "^4.0.2",
    "multer": "^2.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.50",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5"
